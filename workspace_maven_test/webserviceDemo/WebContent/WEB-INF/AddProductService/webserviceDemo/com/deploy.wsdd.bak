<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from AddProductService WSDL service -->

  <service name="AddProduct" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://com.webserviceDemo"/>
      <parameter name="wsdlServiceElement" value="AddProductService"/>
      <parameter name="schemaQualified" value="http://com.webserviceDemo"/>
      <parameter name="wsdlServicePort" value="AddProduct"/>
      <parameter name="className" value="webserviceDemo.com.AddProductSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="AddProduct"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="addProduct" qname="operNS:addProduct" xmlns:operNS="http://com.webserviceDemo" returnQName="retNS:addProductReturn" xmlns:retNS="http://com.webserviceDemo" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:productName" xmlns:pns="http://com.webserviceDemo" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="addProduct"/>

  </service>
</deployment>
